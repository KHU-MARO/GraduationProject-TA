{
  "name": "GraduationProject-TA",
  "version": "1.0.0",
  "description": "The program for Graduation Project",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=public/img/icons/Icon.png.icns --prune=true --out=dist",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=public/img/icons/Icon.ico --prune=true --out=dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sauber92/GraduationProject-TA.git"
  },
  "keywords": [
    "Electron",
    "Arduino",
    "Serialport"
  ],
  "author": "Junyoung Jung(sauber92)",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-installer-dmg": "^0.2.1",
    "electron-rebuild": "^1.4.0",
    "electron-winstaller": "^2.5.2"
  },
  "dependencies": {
    "app": "^0.1.0",
    "electron-packager": "^8.7.0",
    "path": "^0.12.7",
    "serialport": "^4.0.7"
  }
}
